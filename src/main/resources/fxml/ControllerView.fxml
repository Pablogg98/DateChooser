<?xml version="1.0" encoding="UTF-8"?>

<?import dad.javafx.componentes.DateChooser?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>


<GridPane fx:id="view" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
   <children>
      <DateChooser fx:id="dateChooser" alignment="TOP_CENTER" />
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#onIniAction" text="Inicializar" />
            <Button mnemonicParsing="false" onAction="#onConsultarAction" text="Consultar" />
         </children>
      </HBox>
   </children>
   <rowConstraints>
      <RowConstraints vgrow="NEVER" />
      <RowConstraints maxHeight="1.7976931348623157E308" vgrow="ALWAYS" />
   </rowConstraints>
</GridPane>
